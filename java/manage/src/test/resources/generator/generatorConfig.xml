<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<context id="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">
		<property name="beginningDelimiter" value="`" />
		<property name="endingDelimiter" value="`" />

		<plugin type="tk.mybatis.mapper.generator.MapperPlugin">
			<property name="mappers" value="com.wxc.manage.mapper.BaseMapper" />
			<property name="caseSensitive" value="true" />
		</plugin>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://192.168.239.135:3306/manage?&amp;useUnicode=true&amp;characterEncoding=utf-8" userId="root" password="sora0000">
		</jdbcConnection>

		<javaModelGenerator targetPackage="com.wxc.manage.model" targetProject="F:\project\java\manage\src\main\java">
			<!-- rootClass:设置所有实体类的基类。如果设置，需要使用类的全限定名称 -->
			<!-- <property name="rootClass" value="com.xiaoyu.model.BaseObject" /> -->
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="mapper" targetProject="F:\project\java\manage\src\main\resources">
		</sqlMapGenerator>

		<javaClientGenerator targetPackage="com.wxc.manage.mapper" type="XMLMAPPER"
			targetProject="F:\project\java\manage\src\main\java">
		</javaClientGenerator>

		<!-- <table tableName="%"> <generatedKey column="id" sqlStatement="JDBC" /> </table> -->
		<table tableName="%">
			<property name="useActualColumnNames" value="false" />
			<!-- <generatedKey column="id" sqlStatement="SELECT LAST_INSERT_ID()" identity="true" /> -->
		</table>
		<!-- <table schema="dongao" tableName="%"> <property name="useActualColumnNames" value="true" /> <generatedKey column="id" sqlStatement="SELECT LAST_INSERT_ID()" 
			identity="true" /> </table> -->
		<!-- <table schema="dongao" tableName="da_ocs_class" domainObjectName="OcsClass"> <property name="useActualColumnNames" value="true" /> <generatedKey column="id" 
			sqlStatement="SELECT LAST_INSERT_ID()" identity="true" /> </table> -->
	</context>
</generatorConfiguration>